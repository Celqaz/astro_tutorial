---
import type { CollectionEntry } from "astro:content";
import Post from './Post.astro'
type Pros = {
    post: CollectionEntry<'posts'>[];
}

const {posts} = Astro.props;
---


<div class="grid grid-cols-2 gap-x-16 gay-y-14 max-md:grid-cols-1 mb-24">
    {posts.map((post) => (
        <Post post={post} />
    ))}
</div>
